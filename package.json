{
  "name": "archive.hakatashi.com",
  "description": "The private HakataArchive viewer",
  "version": "1.0.0",
  "author": "Koki Takahashi <hakatasiloving@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "deploy": "git checkout gh-pages && git merge master && npm run build && git add . && git commit -m \"Update build\" && git push"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "vue": "^2.6.12"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-json-strings": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@hakatashi/eslint-config": "^1.14.2",
    "babel-loader": "^8.2.1",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.13.0",
    "file-loader": "^6.2.0",
    "node-sass": "^5.0.0",
    "photoswipe": "^4.1.3",
    "sass-loader": "^10.1.0",
    "vue-loader": "^15.9.5",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
