    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@hakatashi/eslint-config": "^1.15.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "eslint": "^7.26.0",
    "file-loader": "^6.2.0",
    "lodash": "^4.17.21",
    "node-sass": "^6.0.0",
    "photoswipe": "^4.1.3",
